<?xml version="1.0" encoding="UTF-8"?>
<mapping xsi:noNamespaceSchemaLocation="..\Mapping.xsd"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:xi="http://www.w3.org/2001/XInclude">

  <!-- Overriding numbers -->
  <mapping name="Devanagari Numbers">
    <entry from="0" to="०" order="20" comment="0" />
    <entry from="1" to="१" order="20" comment="1" />
    <entry from="2" to="२" order="20" comment="2" />
    <entry from="3" to="३" order="20" comment="3" />
    <entry from="4" to="४" order="20" comment="4" />
    <entry from="5" to="५" order="20" comment="5" />
    <entry from="6" to="६" order="20" comment="6" />
    <entry from="7" to="७" order="20" comment="7" />
    <entry from="8" to="८" order="20" comment="8" />
    <entry from="9" to="९" order="20" comment="9" />
  </mapping>
  <mapping name="Arabic Numbers">
    <entry from="Œ" to="0" order="20" comment="0" />
    <entry from="ƒ" to="1" order="20" comment="1" />
    <entry from="„" to="2" order="20" comment="2" />
    <entry from="…" to="3" order="20" comment="3" />
    <entry from="†" to="4" order="20" comment="4" />
    <entry from="‡" to="5" order="20" comment="5" />
    <entry from="ˆ" to="6" order="20" comment="6" />
    <entry from="‰" to="7" order="20" comment="7" />
    <entry from="Š" to="8" order="20" comment="8" />
    <entry from="‹" to="9" order="20" comment="9" />
  </mapping>

  <xi:include href="AARituPlus2.xml"/>
</mapping>
