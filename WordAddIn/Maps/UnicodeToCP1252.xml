<?xml version="1.0" encoding="UTF-8"?>
<!--
  This mapping is meant for converting Unicode codepoints
  that refer to range 0x80-0x9F of the CP1252 codepage back into CP1252.
  This is necessary for TTF/OTF fonts that were generated out of
  Type 1 font. These codes will be further converted in other mappings.

  See:
    - https://www.unicode.org/Public/MAPPINGS/VENDORS/MICSFT/WINDOWS/CP1252.TXT
    - https://www.unicode.org/Public/MAPPINGS/VENDORS/MICSFT/WindowsBestFit/bestfit1252.txt
-->
<mapping xsi:noNamespaceSchemaLocation="Mapping.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <entry from="&#x20AC;" to="&#x80;"/>
  <!-- <entry from="&#x;" to="&#x81;" /> -->
  <entry from="&#x201A;" to="&#x82;"/>
  <entry from="&#x0192;" to="&#x83;"/>
  <entry from="&#x201E;" to="&#x84;"/>
  <entry from="&#x2026;" to="&#x85;"/>
  <entry from="&#x2020;" to="&#x86;"/>
  <entry from="&#x2021;" to="&#x87;"/>
  <entry from="&#x02C6;" to="&#x88;"/>
  <entry from="&#x2030;" to="&#x89;"/>
  <entry from="&#x0160;" to="&#x8A;"/>
  <entry from="&#x2039;" to="&#x8B;"/>
  <entry from="&#x0152;" to="&#x8C;"/>
  <!-- <entry from="&#x;" to="&#x8D;" /> -->
  <entry from="&#x017D;" to="&#x8E;"/>
  <!-- <entry from="&#x;" to="&#x8F;" /> -->
  <!-- <entry from="&#x;" to="&#x90;" /> -->
  <entry from="&#x2018;" to="&#x91;"/>
  <entry from="&#x2019;" to="&#x92;"/>
  <entry from="&#x201C;" to="&#x93;"/>
  <entry from="&#x201D;" to="&#x94;"/>
  <entry from="&#x2022;" to="&#x95;"/>
  <entry from="&#x2013;" to="&#x96;"/>
  <entry from="&#x2014;" to="&#x97;"/>
  <entry from="&#x02DC;" to="&#x98;"/>
  <entry from="&#x2122;" to="&#x99;"/>
  <entry from="&#x0161;" to="&#x9A;"/>
  <entry from="&#x203A;" to="&#x9B;"/>
  <entry from="&#x0153;" to="&#x9C;"/>
  <!-- <entry from="&#x;" to="&#x9D;" /> -->
  <entry from="&#x017E;" to="&#x9E;"/>
  <entry from="&#x0178;" to="&#x9F;"/>
</mapping>
